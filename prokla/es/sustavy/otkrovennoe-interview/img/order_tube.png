    <script>
        function scrollToForm() {
            let link = $('a[href*="#form__block"]');
            let form = $('#form__block');
        
            if ($(window).width() > 800) {
                offset = 80
            } else if ($(window).width() <= 799) {
                offset = 50
            }
            
            link.on('click', function() {
                setTimeout(function() {
                    $('html, body').stop().animate({
                        scrollTop: $(form).offset().top - offset
                    }, 777);
                }, 0);
            
            });
        }
        scrollToForm();
    </script>     
