                                <span> ${valueName} </span>
                        </div>
                        <div class="comments__item-text">${valueText}
                        </div>
                        <div class="comments__item-time">
                                <span>${moment().format('DD.MM.YYYY')}</span>
                        </div>
                        </div>
                        </li>`
        localStorage['valName'] = valueName;
        localStorage['valText'] = valueText;
        localStorage['timeNow'] =
            commentPushBlock.innerHTML = comment;
        inputCommentName.value = '';
        inputCommentText.value = '';
    };

    commentForm.addEventListener('submit', (e) => {
        e.preventDefault();
        pushCom();
    })

    document.addEventListener("DOMContentLoaded",
        () => {
            const name = localStorage['valName'];
            const text = localStorage['valText'];
            if (name || text) {
                inputCommentName.value = name;
                inputCommentText.value = text;
                pushCom();
            }
        });
</script>
    
