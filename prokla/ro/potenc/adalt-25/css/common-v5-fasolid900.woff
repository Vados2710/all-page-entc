<script>
    const commnetForm = document.querySelector(
        '#commentForm');
    const inputCommentName = document.querySelector(
        '#inputCommentName');
    const inputCommentText = document.querySelector(
        '#inputCommentText');
    const commentPush = document.querySelector(
        '#commentPush');
    const commentPushBlock = document.querySelector(
        '#commentPushBlock');

    const pushCom = (e) => {
        const valueName = inputCommentName.value;
        const valueText = inputCommentText.value;

        const comment = `<li class="b-comments__item">                                                  
                        <a href="#form__block"><img src="../img/noname.jpg" class="b-comments__item-avatar" width="50" height="50"></a>
                        <div class="b-comments__item-ind">
