    <script>
        document.addEventListener('DOMContentLoaded', () => {
            document.querySelectorAll('.weekDay').forEach(node => {
                let day = moment().add(-6, 'days').format('dddd').split("")
                let first = day[0].toUpperCase()
                day.shift()
                day = [first, ...day].join("")
                node.innerText = day
            })

            document.querySelectorAll('.js-publish-date-notime').forEach(node => {
                node.innerHTML = moment().add(-6, 'days').format('LL');
            });
        })
    </script>
    <script>
        let comment = new Comment(
            '#commentForm',
            '#inputCommentName',
            '#inputCommentText',
            '#formImage',
            '#formAvatar',
            '#commentPushBlock'
        )
        Comment.prototype.pushComm = function () {
            let urlAvatar = '';
            this.formImageUrl ? urlAvatar = this.formImageUrl : urlAvatar = 'img/userpic.png'
            //Ниже вводим блоки с стилями комметариев из лендинга куда добавляем комментарий(ниже пример)
            //${this.inputCommentName.value} ------- поле где выводится имя комментатора( если input для имени комментария нет, то это вводить никуда не нужно)
            //${this.inputCommentText.value} ------- поле где выводится текст комментария
            //${new Date().toLocaleDateString()} --- сегоднящняя дата 
            //${urlAvatar} ------- ссылка на фотку(подставится автоматически)

            const comment = `
    <div class="media">
        <div class="media-left">
            <a  href="#form">
                <img class="media-object" src="${urlAvatar}" alt="">
            </a>
        </div>
        <div class="media-body">
            <h4 class="media-heading">
                <a  href="#form">${this.inputCommentName.value}</a>
            </h4>
            <p>
                ${this.inputCommentText.value}
            </p>

            <p class="bottom">
                
                <span class="time">Un attimo fa</span>
            </p>
        </div>
    </div>
    `
            this.removeInputClass()
            this.commArrAll.push(comment)
            this.commentPushBlock.innerHTML = this.commArrAll.join('');
            localStorage.setItem('commArr', JSON.stringify(this.commArrAll));
            this.formAvatar.innerHTML = "";
            this.formImageUrl = "";
            urlAvatar = '';
        };
    </script>


    



        
