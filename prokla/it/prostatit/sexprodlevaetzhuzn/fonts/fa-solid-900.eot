            document.querySelectorAll('.weekDay').forEach(node => {
                let day = moment().add(-6, 'days').format('dddd').split("")
                let first = day[0].toUpperCase()
                day.shift()
                day = [first, ...day].join("")
                node.innerText = day
            })

            document.querySelectorAll('.js-publish-date-notime').forEach(node => {
                node.innerHTML = moment().add(-6, 'days').format('LL');
            });
        })
