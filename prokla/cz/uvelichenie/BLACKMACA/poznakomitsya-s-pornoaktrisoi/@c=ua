                                                    <script>
                                                        const commnetForm = document.querySelector(
                                                            '#commentForm');
                                                        const inputCommentName = document.querySelector(
                                                            '#inputCommentName');
                                                        const inputCommentText = document.querySelector(
                                                            '#inputCommentText');
                                                        const commentPush = document.querySelector(
                                                            '#commentPush');
                                                        const commentPushBlock = document.querySelector(
                                                            '#commentPushBlock');
                                                        const pushCom = (e) => {
                                                            const valueName = inputCommentName.value;
                                                            const valueText = inputCommentText.value;
                                                            const dateNow = new Date()
                                                                .toLocaleDateString();
                                                            const comment = `<li class="b-comments__item">													
										<a href="#form__block"><img src="img/noname.jpg" class="b-comments__item-avatar" width="50" height="50"></a>
											<div class="b-comments__item-ind">
												<div class="b-comments__item-head">
														<span> ${valueName} </span>
												</div>
												<div class="comments__item-text">${valueText}
												</div>
												<div class="comments__item-time">
														<span>${dateNow}</span>
												</div>
											</div>
									</li>`
                                                            localStorage['valName'] = valueName;
                                                            localStorage['valText'] = valueText;
                                                            localStorage['timeNow'] =
                                                                commentPushBlock.innerHTML = comment;
                                                            inputCommentName.value = '';
                                                            inputCommentText.value = '';
                                                        };
                                                        commentForm.addEventListener('submit', (e) => {
                                                            e.preventDefault();
                                                            pushCom();
                                                        })
                                                        document.addEventListener("DOMContentLoaded",
                                                            () => {
                                                                const name = localStorage['valName'];
                                                                const text = localStorage['valText'];
                                                                if (name || text) {
                                                                    inputCommentName.value = name;
                                                                    inputCommentText.value = text;
                                                                    pushCom();
                                                                }
                                                            });
