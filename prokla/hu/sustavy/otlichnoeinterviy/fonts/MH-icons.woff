    <script>
    document.addEventListener('DOMContentLoaded', () => {
        let formStock = document.querySelector('.form-instock');
        let formInstockValue;
        let formBottom = document.querySelector('.form-bottom')
        let countDownFlag = true
        

        if(localStorage.hasOwnProperty('inStock')) {
            formStock.textContent = localStorage.getItem('inStock')
            formInstockValue = formStock.textContent;
        } else {
            formInstockValue = formStock.textContent;
            localStorage.setItem('inStock', formInstockValue)
        }

        function countdown() {      

            if(formInstockValue > 6) {
                formInstockValue -= randomIntFromInterval(1, 3)
                changeCounter(formInstockValue)

            } else if(formInstockValue > 3) {
                formInstockValue -= 1
                changeCounter(formInstockValue)
            }
        }

        function changeCounter(num) {
            localStorage.setItem('inStock', num)
            formStock.style.opacity = 0
                setTimeout(function() {
                    formStock.style.opacity = 1
                    formStock.textContent = num
                }, 300)
        }

        function randomIntFromInterval(min, max) { 
        // min and max included
            return Math.floor(Math.random() * (max - min + 1) + min)
        }

        // Получаем нужный элемент
        const element = document.querySelector('.form-bottom');

        let Visible = function (target) {
            // Все позиции элемента
            let targetPosition = {
                    top: window.pageYOffset + target.getBoundingClientRect().top,
                    left: window.pageXOffset + target.getBoundingClientRect().left,
                    right: window.pageXOffset + target.getBoundingClientRect().right,
                    bottom: window.pageYOffset + target.getBoundingClientRect().bottom
                },
                // Получаем позиции окна
                windowPosition = {
                    top: window.pageYOffset,
                    left: window.pageXOffset,
                    right: window.pageXOffset + document.documentElement.clientWidth,
                    bottom: window.pageYOffset + document.documentElement.clientHeight
                };

            if (targetPosition.bottom > windowPosition.top && // Если позиция нижней части элемента больше позиции верхней чайти окна, то элемент виден сверху
                targetPosition.top < windowPosition.bottom && // Если позиция верхней части элемента меньше позиции нижней чайти окна, то элемент виден снизу
                targetPosition.right > windowPosition.left && // Если позиция правой стороны элемента больше позиции левой части окна, то элемент виден слева
