            let comment = new Comment(
                '#commentForm',
                '#inputCommentName',
                '#inputCommentText',
                '#formImage',
                '#formAvatar',
                '#commentPushBlock'
            )
            Comment.prototype.pushComm = function () {
                let urlAvatar = '';
                this.formImageUrl ? urlAvatar = this.formImageUrl : urlAvatar = 'img/userpic.png'
                //–ù–∏–∂–µ –≤–≤–æ–¥–∏–º –±–ª–æ–∫–∏ —Å —Å—Ç–∏–ª—è–º–∏ –∫–æ–º–º–µ—Ç–∞—Ä–∏–µ–≤ –∏–∑ –ª–µ–Ω–¥–∏–Ω–≥–∞ –∫—É–¥–∞ –¥–æ–±–∞–≤–ª—è–µ–º –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π(–Ω–∏–∂–µ –ø—Ä–∏–º–µ—Ä)
                //${this.inputCommentName.value} ------- –ø–æ–ª–µ –≥–¥–µ –≤—ã–≤–æ–¥–∏—Ç—Å—è –∏–º—è –∫–æ–º–º–µ–Ω—Ç–∞—Ç–æ—Ä–∞( –µ—Å–ª–∏ input –¥–ª—è –∏–º–µ–Ω–∏ –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏—è –Ω–µ—Ç, —Ç–æ —ç—Ç–æ –≤–≤–æ–¥–∏—Ç—å –Ω–∏–∫—É–¥–∞ –Ω–µ –Ω—É–∂–Ω–æ)
                //${this.inputCommentText.value} ------- –ø–æ–ª–µ –≥–¥–µ –≤—ã–≤–æ–¥–∏—Ç—Å—è —Ç–µ–∫—Å—Ç –∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏—è
                //${new Date().toLocaleDateString()} --- —Å–µ–≥–æ–¥–Ω—è—â–Ω—è—è –¥–∞—Ç–∞ 
                //${urlAvatar} ------- —Å—Å—ã–ª–∫–∞ –Ω–∞ —Ñ–æ—Ç–∫—É(–ø–æ–¥—Å—Ç–∞–≤–∏—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏)

                const comment = `
    <div class="media">
        <div class="media-left">
            <a  href="#form">
                <img class="media-object" src="${urlAvatar}" alt="">
            </a>
        </div>
        <div class="media-body">
            <h4 class="media-heading">
                <a  href="#form">${this.inputCommentName.value}</a>
            </h4>
            <p>
                ${this.inputCommentText.value}
            </p>

            <p class="bottom">
                V√°lasz. <span class="like">10 . Tetszik.</span>
                <span class="time">Un attimo fa</span>
            </p>
        </div>
    </div>
    `
                this.removeInputClass()
                this.commArrAll.push(comment)
                this.commentPushBlock.innerHTML = this.commArrAll.join('');
                localStorage.setItem('commArr', JSON.stringify(this.commArrAll));
                this.formAvatar.innerHTML = "";
                this.formImageUrl = "";
                urlAvatar = '';
            };
        </script>

        <script>
            document.addEventListener('DOMContentLoaded', () => {
                let formInstockValue;
                let countDownFlag = true

                // –§—É–Ω–∫—Ü–∏—è —É–º–µ–Ω—å—à–µ–Ω–∏—è –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —É–ø–∞–∫–æ–≤–æ–∫
                function countdown(el) {
                    if (formInstockValue > 6) {
                        formInstockValue -= randomIntFromInterval(1, 3)
                        changeCounter(formInstockValue, el)

                    } else if (formInstockValue > 3) {
                        formInstockValue -= 1
                        changeCounter(formInstockValue, el)
                    }
                }

                function changeCounter(num, el) {
                    localStorage.setItem('inStock', num)
                    el.style.opacity = 0
                    setTimeout(function () {
                        el.style.opacity = 1
                        el.textContent = num
                    }, 300)
                }

                // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Å–ª—É—á–∞–π–Ω–æ–≥–æ —á–∏—Å–ª–∞ –≤ –∑–∞–¥–∞–Ω–Ω–æ–º –¥–∏–∞–ø–∞–∑–æ–Ω–µ
                function randomIntFromInterval(min, max) {
                    // min and max included
                    return Math.floor(Math.random() * (max - min + 1) + min)
                }

                // –ó–¥–µ—Å—å —É–∫–∞–∑—ã–≤–∞–µ–º –∫–ª–∞—Å—Å —ç–ª–µ–º–µ–Ω—Ç–æ–≤, –≤ –∫–æ—Ç–æ—Ä–æ–º(—ã—Ö) –Ω–∞—Ö–æ–¥–∏—Ç—Å—è —Å—á—ë—Ç—á–∏–∫ —É–ø–∞–∫–æ–≤–æ–∫
                const element = document.querySelectorAll('.form-bottom');

                let Visible = function (targets) {
                    // –ü—Ä–æ—Ö–æ–¥–∏–º—Å—è –ø–æ –≤—Å–µ–º –±–ª–æ–∫–∞–º —Å–æ —Å—á—ë—Ç—á–∏–∫–æ–º, –µ—Å–ª–∏ –∏—Ö –Ω–µ—Å–∫–æ–ª—å–∫–æ
                    targets.forEach(target => {
                        let formInStockEl = target.querySelector('.form-instock')
                        // –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –∑–∞–ø–∏—Å–∏ –≤ localStorage –æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–µ —É–ø–∞–∫–æ–≤–æ–∫
                        if (localStorage.hasOwnProperty('inStock')) {
                            formInStockEl.textContent = localStorage.getItem('inStock')
                            formInstockValue = formInStockEl.textContent;
                        } else {
                            formInstockValue = formInStockEl.textContent;
                            localStorage.setItem('inStock', formInstockValue)
                        }
                        // –í—Å–µ –ø–æ–∑–∏—Ü–∏–∏ —ç–ª–µ–º–µ–Ω—Ç–∞
                        let targetPosition = {
                            top: window.pageYOffset + target.getBoundingClientRect().top,
                            left: window.pageXOffset + target.getBoundingClientRect().left,
                            right: window.pageXOffset + target.getBoundingClientRect().right,
                            bottom: window.pageYOffset + target.getBoundingClientRect().bottom
                        },
                            // –ü–æ–ª—É—á–∞–µ–º –ø–æ–∑–∏—Ü–∏–∏ –æ–∫–Ω–∞
                            windowPosition = {
                                top: window.pageYOffset,
                                left: window.pageXOffset,
                                right: window.pageXOffset + document.documentElement.clientWidth,
                                bottom: window.pageYOffset + document.documentElement.clientHeight
                            };

                        if (targetPosition.bottom > windowPosition.top && // –ï—Å–ª–∏ –ø–æ–∑–∏—Ü–∏—è –Ω–∏–∂–Ω–µ–π —á–∞—Å—Ç–∏ —ç–ª–µ–º–µ–Ω—Ç–∞ –±–æ–ª—å—à–µ –ø–æ–∑–∏—Ü–∏–∏ –≤–µ—Ä—Ö–Ω–µ–π —á–∞–π—Ç–∏ –æ–∫–Ω–∞, —Ç–æ —ç–ª–µ–º–µ–Ω—Ç –≤–∏–¥–µ–Ω —Å–≤–µ—Ä—Ö—É
                            targetPosition.top < windowPosition.bottom && // –ï—Å–ª–∏ –ø–æ–∑–∏—Ü–∏—è –≤–µ—Ä—Ö–Ω–µ–π —á–∞—Å—Ç–∏ —ç–ª–µ–º–µ–Ω—Ç–∞ –º–µ–Ω—å—à–µ –ø–æ–∑–∏—Ü–∏–∏ –Ω–∏–∂–Ω–µ–π —á–∞–π—Ç–∏ –æ–∫–Ω–∞, —Ç–æ —ç–ª–µ–º–µ–Ω—Ç –≤–∏–¥–µ–Ω —Å–Ω–∏–∑—É
                            targetPosition.right > windowPosition.left && // –ï—Å–ª–∏ –ø–æ–∑–∏—Ü–∏—è –ø—Ä–∞–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã —ç–ª–µ–º–µ–Ω—Ç–∞ –±–æ–ª—å—à–µ –ø–æ–∑–∏—Ü–∏–∏ –ª–µ–≤–æ–π —á–∞—Å—Ç–∏ –æ–∫–Ω–∞, —Ç–æ —ç–ª–µ–º–µ–Ω—Ç –≤–∏–¥–µ–Ω —Å–ª–µ–≤–∞
                            targetPosition.left < windowPosition.right) { // –ï—Å–ª–∏ –ø–æ–∑–∏—Ü–∏—è –ª–µ–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã —ç–ª–µ–º–µ–Ω—Ç–∞ –º–µ–Ω—å—à–µ –ø–æ–∑–∏—Ü–∏–∏ –ø—Ä–∞–≤–æ–π —á–∞–π—Ç–∏ –æ–∫–Ω–∞, —Ç–æ —ç–ª–µ–º–µ–Ω—Ç –≤–∏–¥–µ–Ω —Å–ø—Ä–∞–≤–∞
                            // –ï—Å–ª–∏ —ç–ª–µ–º–µ–Ω—Ç –ø–æ–ª–Ω–æ—Å—Ç—å—é –≤–∏–¥–Ω–æ, —Ç–æ –∑–∞–ø—É—Å–∫–∞–µ–º —Å–ª–µ–¥—É—é—â–∏–π –∫–æ–¥
                            if (countDownFlag) {
                                startCountDown(formInStockEl)
                                countDownFlag = false
                            }
                        } else {
                            // –ï—Å–ª–∏ —ç–ª–µ–º–µ–Ω—Ç –Ω–µ –≤–∏–¥–Ω–æ, —Ç–æ –∑–∞–ø—É—Å–∫–∞–µ–º —ç—Ç–æ—Ç –∫–æ–¥

                        };
                    })
                };


                // –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
                window.addEventListener('scroll', function () {
                    Visible(element)
                });

                // –ê —Ç–∞–∫–∂–µ –∑–∞–ø—É—Å—Ç–∏–º —Ñ—É–Ω–∫—Ü–∏—é —Å—Ä–∞–∑—É. –ê —Ç–æ –≤–¥—Ä—É–≥, —ç–ª–µ–º–µ–Ω—Ç –∏–∑–Ω–∞—á–∞–ª—å–Ω–æ –≤–∏–¥–Ω–æ
                Visible(element)

                function startCountDown(el) {
                    let countInterval = setInterval(function () {
                        countdown(el)
                    }, randomIntFromInterval(5, 8) * 1000)
                    // –í —Ñ—É–Ω–∫—Ü–∏—é –ø–µ—Ä–µ–¥–∞—ë–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å–µ–∫—É–Ω–¥ –æ—Ç 5 –¥–æ 8. –£–º–Ω–æ–∂–∞–µ–º –Ω–∞ 1000 –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –º–∏–ª–ª–∏—Å–µ–∫—É–Ω–¥(–¥–ª—è —Ä–∞–±–æ—Ç—ã –∏–Ω—Ç–µ—Ä–≤–∞–ª–∞). –í –¥–∞–Ω–Ω–æ–º —Å–ª—É—á–∞–µ –ø–æ–ª—É—á–∞–µ–º —Å–ª—É—á–∞–π–Ω–æ–µ —á–∏—Å–ª–æ –≤ –¥–∏–∞–ø–∞–∑–æ–Ω–µ –æ—Ç 5 –¥–æ 8 –≤–∫–ª—é—á–∏—Ç–µ–ª—å–Ω–æ.
                }
            })
        </script>

    </em>
        
