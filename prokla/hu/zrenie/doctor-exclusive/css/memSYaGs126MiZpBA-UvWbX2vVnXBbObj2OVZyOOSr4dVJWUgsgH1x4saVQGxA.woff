  <script>
    let numClass = ".form-instock";
    let countInterval, counter = document.querySelector(numClass),
      scrollBlock = document.querySelector(numClass),
      count = 27,
      timeInterval = 8e3,
      counterStart = !1;

    let counters = document.querySelectorAll(numClass);

    counter.style.transition = "../opacity .4s"

    localStorage.countNum ? counters.forEach(i => i.textContent = localStorage.countNum) : (localStorage.countNum =
      count, counters.forEach(i => i.textContent = localStorage.countNum));

    let randomNum = (t, o) => Math.floor(Math.random() * o + t),
      getNum = (t, o) => {
        counters.forEach(i => i.style.opacity = 0)
        setTimeout(() => {
          localStorage.countNum = localStorage.countNum - randomNum(t, o)
          counters.forEach(i => (i.textContent = localStorage.countNum, i.style.opacity = 1))
        }, 600)
      },
      go = () => +localStorage.countNum > 3 && !counterStart ? (countInterval = setInterval(() => localStorage
        .countNum > 11 ? getNum(1, 3) : localStorage.countNum > 7 ? getNum(1, 1) : null, timeInterval),
        counterStart = !0) : null,
      stop = () => (clearInterval(countInterval), counterStart = !1);

    document.querySelector("body").clientHeight <= window.innerHeight ? go() : window.addEventListener("scroll",
      () => window.scrollY + window.innerHeight > scrollBlock.offsetTop || !window.scrollY ? go() : stop());
  </script>

      
